[display]

window/stretch/mode="canvas_items"
window/stretch/aspect="ignore"

[editor]

script/templates_search_path="res://Utils/ScriptTemplates"

[editor_plugins]

enabled=PackedStringArray("res://addons/gs_logger/plugin.cfg", "res://addons/gut/plugin.cfg", "res://addons/integrodot/plugin.cfg", "res://addons/shaker/plugin.cfg")

